/// Struct for string manipulation
pub struct String {};

impl String {
    pub fn new() -> string {
        return "";
    }

    // TODO: refactor this to use match in future
    pub fn to_bool(s: string) -> bool? {
        if s == "true" {
            return true;
        } else if s == "false" {
            return false;
        } else {
            throw "provided string is not a boolean";
        }
    }
}